{
	"name": "canvas-drawing",
	"version": "0.1.0",
	"private": true,
	"lint-staged": {
		"*.{js,jsx,json,css}": [
			"npm run prettify:fix"
		]
	},
	"scripts": {
		"prepare": "husky install",
		"start": "react-scripts start",
		"build": "react-scripts build",
		"test": "react-scripts test",
		"eject": "react-scripts eject",
		"lint": "eslint --ignore-path .gitignore ./src",
		"lint:fix": "npm run lint --fix",
		"prettify": "prettier . -c --config .prettierrc",
		"prettify:fix": "prettier . -c --config .prettierrc --write",
		"build:umd": "rm -rf lib && NODE_ENV=production webpack --config webpack.prod.config.js",
		"build:umd:withAnalyzer": "NODE_ENV=production ANALYZER=true webpack --config webpack.prod.config.js"
	},
	"dependencies": {},
	"devDependencies": {
		"@testing-library/jest-dom": "^5.16.3",
		"@testing-library/react": "^12.1.4",
		"@testing-library/user-event": "^13.5.0",
		"@types/jest": "^27.4.1",
		"@types/node": "^16.11.26",
		"@types/react": "^17.0.43",
		"@types/react-dom": "^17.0.14",
		"@typescript-eslint/eslint-plugin": "^5.17.0",
		"@typescript-eslint/parser": "^5.17.0",
		"autoprefixer": "10.4.4",
		"babel-plugin-transform-class-properties": "6.24.1",
		"css-loader": "6.7.1",
		"eslint": "^7.32.0",
		"eslint-config-standard": "^16.0.3",
		"eslint-plugin-import": "^2.25.4",
		"eslint-plugin-node": "^11.1.0",
		"eslint-plugin-prettier": "^4.0.0",
		"eslint-plugin-promise": "^5.2.0",
		"eslint-plugin-react": "^7.29.4",
		"husky": "^7.0.4",
		"lint-staged": "^12.3.7",
		"postcss-loader": "6.2.1",
		"prettier": "2.6.1",
		"react-scripts": "5.0.0",
		"sass": "^1.49.11",
		"sass-loader": "12.6.0",
		"terser-webpack-plugin": "5.3.1",
		"ts-loader": "9.2.8",
		"typescript": "4.6.3",
		"web-vitals": "^2.1.4",
		"webpack-bundle-analyzer": "4.5.0",
		"webpack-cli": "4.9.2"
	},
	"peerDependencies": {
		"react": "17-18",
		"react-dom": "17-18"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	}
}
